<% if user_signed_in? %>
  <%= link_to "Sign out", destroy_user_session_path, method: :delete, class: "btn btn-outline-success my-2 my-sm-0", role: "button" %>
  <%= link_to "Settings", "#", class: "btn btn-outline-success my-2 my-sm-0", role: "button" %>
<% else %>
  <%= link_to "Sign in", new_user_session_path, class: "btn btn-outline-success my-2 my-sm-0", role: "button" %>
  <%= link_to "Sign up", new_user_registration_path, class: "btn btn-outline-success my-2 my-sm-0", role: "button" %>
<% end %>

